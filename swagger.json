    "components": {
      "schemas": {
        "RegistrationRequest": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string"
            },
            "email": {
              "type": "string"
            },
            "gender": {
              "type": "string"
            },
            "password": {
              "type": "string"
            },
            "phone": {
              "type": "string"
            }
          },
          "required": [
            "name",
            "email",
            "gender",
            "password",
            "phone"
          ],
          "additionalProperties": false
        },
        "LoginRequest": {
          "type": "object",
          "properties": {
            "email": {
              "type": "string"
            },
            "password": {
              "type": "string"
            }
          },
          "required": [
            "email",
            "password"
          ],
          "additionalProperties": false
        },
        "DeleteResponse": {
          "type": "object",
          "properties": {
            "status": {
              "type": "string"
            },
            "message": {
              "type": "string"
            }
          }
        },
        "TokenResonse": {
          "type": "object",
          "properties": {
            "auth": {
              "type": "boolean"
            },
            "token": {
              "type": "string"
            }
          }
        },
        "UserProfile": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string"
            },
            "email": {
              "type": "string"
            },
            "gender": {
              "type": "string"
            },
            "phone": {
              "type": "string"
            }
          }
        }
      }
    }
